{
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^\\.rpl/build/install_buildx\\.sh$"],
      "matchStrings": ["export BUILDX_VERSION=(v\\d+\\.\\d+\\.\\d+)"],
      "depNameTemplate": "docker/buildx",
      "datasourceTemplate": "github-releases"
    }
  ],  
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "prHeader": "JamesTestHeader",
  "assignees": [
    "james-willett"
  ],
  "labels": ["dependencies-testlabel"],
  "packageRules": [
    {
      "matchPackagePatterns": ["jackson"],
      "labels": ["jackson-testlabel"]
    },
    {
      "matchPackagePatterns": ["maven"],
      "labels": ["maven-testlabel"]
    }
  ]
}
